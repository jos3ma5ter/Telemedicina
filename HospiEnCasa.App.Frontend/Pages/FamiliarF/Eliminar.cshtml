@page
@model HospiEnCasa.App.Frontend.Pages.FamiliarF.EliminarModel
@{
}
<div class="formularios">
    <div class="espacio">
        <h1>Eliminar Familiar</h1>

        <form method="post">
            <div class="form-group">
                <label asp-for="familiar.Id"></label>
                <input asp-for="familiar.Id" class="form-control" readonly>
            </div>
            <div class="form-group">
                <label asp-for="familiar.Nombres"></label>
                <input asp-for="familiar.Nombres" class="form-control" required>
            </div>
            <button type="submit" class="btn btn-primary">Continuar</button>
        </form>
    </div>
</div>